<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
         http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">

  <display-name>Developer Information</display-name>

  <servlet>
    <servlet-name>DeveloperUpdate</servlet-name>
    <servlet-class>com.newbei.developerInfoWeb.controller.servlets.UpdateDeveloper</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>com.newbei.developerInfoWeb.controller.servlets.LoginServlet</servlet-class>
  </servlet>
  <servlet>
      <servlet-name>SaveDeveloper</servlet-name>
      <servlet-class>com.newbei.developerInfoWeb.controller.servlets.SaveDeveloper</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SaveDeveloper</servlet-name>
    <url-pattern>/saveDeveloper</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DeveloperUpdate</servlet-name>
    <url-pattern>/updateDeveloper</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>

</web-app>
